<syncfusion:ChromelessWindow
        x:Class="BeeSSH.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BeeSSH"
        xmlns:syncfusion="http://schemas.syncfusion.com/wpf" 
        xmlns:syncfusionskin="clr-namespace:Syncfusion.SfSkinManager;assembly=Syncfusion.SfSkinManager.WPF"
        Title="BeeSSH" Height="600" Width="1250"
        WindowStartupLocation="CenterScreen"
        syncfusionskin:SfSkinManager.Theme="{syncfusionskin:SkinManagerExtension ThemeName=MaterialDark, FluentHoverEffectMode=Border}">
    <Grid Background="#FF272A3A">

        <!--Login Frame-->
        <Frame Source="/UserFrames/Login.xaml" x:Name="LoginFrame" Width="800" Margin="225,0,225,0" Height="585"/>

    </Grid>
</syncfusion:ChromelessWindow>
